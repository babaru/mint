
<%= simple_form_for(@overtime_record , html: {class: 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
            
    <%= f.input :user_id %>
          
    <%= f.input :happened_at %>
          
    <%= f.input :value %>
                    </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
