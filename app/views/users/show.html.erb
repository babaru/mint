<div class="narrow-center-page">
<%= render_cell(
  :page_title,
  :show,
  data:
  {
    name: "#{@user.name}",
    toolbar: {
      left:
      [
        render_cell(
          :button,
          :show,
          data:
          {
            name: fa_icon('level-up rotate-270'),
            url: users_path,
            options:
            {
              class: 'btn',
              rel: 'tooltip',
              title: "#{t('labels.back_to')}#{t('model.list', :model => User.model_name.human)}",
            }
          }
        )
      ]
    }
  }
)
%>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>
        属性
      </th>
      <th>
        值
      </th>
    </tr>
  </thead>
  <tbody>
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.email') %>
      </td>
      <td>
        <%= @user.email %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.encrypted_password') %>
      </td>
      <td>
        <%= @user.encrypted_password %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.reset_password_token') %>
      </td>
      <td>
        <%= @user.reset_password_token %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.reset_password_sent_at') %>
      </td>
      <td>
        <%= @user.reset_password_sent_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.remember_created_at') %>
      </td>
      <td>
        <%= @user.remember_created_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.sign_in_count') %>
      </td>
      <td>
        <%= @user.sign_in_count %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.current_sign_in_at') %>
      </td>
      <td>
        <%= @user.current_sign_in_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.last_sign_in_at') %>
      </td>
      <td>
        <%= @user.last_sign_in_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.current_sign_in_ip') %>
      </td>
      <td>
        <%= @user.current_sign_in_ip %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.last_sign_in_ip') %>
      </td>
      <td>
        <%= @user.last_sign_in_ip %>
      </td>
    </tr>
    
        
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.user.name') %>
      </td>
      <td>
        <%= @user.name %>
      </td>
    </tr>
    
    
  </tbody>
  <tfoot>

  </tfoot>
</table>
</div>
