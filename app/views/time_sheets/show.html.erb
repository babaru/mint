<div class="narrow-center-page">
<%= render_cell(
  :page_title,
  :show,
  data:
  {
    name: "#{@time_sheet.name}",
    toolbar: {
      left:
      [
        render_cell(
          :button,
          :show,
          data:
          {
            name: fa_icon('level-up rotate-270'),
            url: time_sheets_path,
            options:
            {
              class: 'btn',
              rel: 'tooltip',
              title: "#{t('labels.back_to')}#{t('model.list', :model => TimeSheet.model_name.human)}",
            }
          }
        )
      ]
    }
  }
)
%>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>
        属性
      </th>
      <th>
        值
      </th>
    </tr>
  </thead>
  <tbody>
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.user_id') %>
      </td>
      <td>
        <%= @time_sheet.user_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.project_id') %>
      </td>
      <td>
        <%= @time_sheet.project_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.recorded_hours') %>
      </td>
      <td>
        <%= @time_sheet.recorded_hours %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.overtime_hours') %>
      </td>
      <td>
        <%= @time_sheet.overtime_hours %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.calculated_hours') %>
      </td>
      <td>
        <%= @time_sheet.calculated_hours %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.recorded_on') %>
      </td>
      <td>
        <%= @time_sheet.recorded_on %>
      </td>
    </tr>
    
        
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.calculated_normal_hours') %>
      </td>
      <td>
        <%= @time_sheet.calculated_normal_hours %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.time_sheet.calculated_overtime_hours') %>
      </td>
      <td>
        <%= @time_sheet.calculated_overtime_hours %>
      </td>
    </tr>
    
    
  </tbody>
  <tfoot>

  </tfoot>
</table>
</div>
