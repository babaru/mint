    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.started_at') %>
      </td>
      <td>
        <%= @leave_record.started_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.ended_at') %>
      </td>
      <td>
        <%= @leave_record.ended_at %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.type') %>
      </td>
      <td>
        <%= @leave_record.type %>
      </td>
    </tr>
    
    
  </tbody>
  <tfoot>

  </tfoot>
</table>
</div>
