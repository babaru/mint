<div class="narrow-center-page">
<%= render_cell(
  :page_title,
  :show,
  data:
  {
    name: "#{@leave_record.name}",
    toolbar: {
      left:
      [
        render_cell(
          :button,
          :show,
          data:
          {
            name: fa_icon('level-up rotate-270'),
            url: leave_records_path,
            options:
            {
              class: 'btn',
              rel: 'tooltip',
              title: "#{t('labels.back_to')}#{t('model.list', :model => LeaveRecord.model_name.human)}",
            }
          }
        )
      ]
    }
  }
)
%>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>
        属性
      </th>
      <th>
        值
      </th>
    </tr>
  </thead>
  <tbody>
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.user_id') %>
      </td>
      <td>
        <%= @leave_record.user_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.project_id') %>
      </td>
      <td>
        <%= @leave_record.project_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.recorded_on') %>
      </td>
      <td>
        <%= @leave_record.recorded_on %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.value') %>
      </td>
      <td>
        <%= @leave_record.value %>
      </td>
    </tr>
    
        
        
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.task_type_id') %>
      </td>
      <td>
        <%= @leave_record.task_type_id %>
      </td>
    </tr>
    
        
    <tr>
      <td>
        <%= t('activerecord.attributes.leave_record.remark') %>
